<Window x:Class="Testiki.TestikiOkno"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Testiki"
        mc:Ignorable="d"
        Title="TestikiOkno" Height="450" Width="800">
    <Grid >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.3*"/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.11*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="ExitBtm" Content="Выйти" Click="ExitBtm_Click"/>
        <Button x:Name="RedactorTestBtm" Content="Редактировать тест" Grid.Column="1" Click="RedactorTestBtm_Click"/>
        <Button x:Name="ProhodTestBtm" Content="Пройти тест" Grid.Column="2" Click="ProhodTestBtm_Click"/>
        <Frame x:Name="FramePag" Grid.Row="1" Grid.ColumnSpan="3"/>
    </Grid>
</Window>
